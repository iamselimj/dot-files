# File and directory handling
alias l="ls"
alias ll="ls -l"
alias la="ls -la"
alias lt="ls --human-readable --size -1 -S --classify"
alias rm="rm -v"
alias filetree="find . -print | sed -e 's;[^/]*/;|____;g;s;____|; |;g'"
alias count='find . -type f | wc -l'
alias vi=vim
alias c="clear"
alias j="jobs -l"
